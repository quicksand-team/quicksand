              "type": ["boolean", "array"]
            },
            "peerDependencies": {
              "type": ["boolean", "array"]
            },
            "bundledDependencies": {
              "type": ["boolean", "array"]
            },
            "packageDir": {
              "type": ["string", "array"]
            },
            "includeInternal": {
              "type": ["boolean"]
            },
            "includeTypes": {
              "type": ["boolean"]
            }
          },
          "additionalProperties": false
        }
      ]
    }
  },
